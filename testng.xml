<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="LambdaTestSuite" parallel="tests" thread-count="2">
    <test name="Chrome128Win10">
        <parameter name="browser" value="Chrome"/>
        <parameter name="version" value="128.0"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="Capabilities.TestScenario1"/>
        </classes>
    </test>
    <test name="Edge127Mac">
        <parameter name="browser" value="MicrosoftEdge"/>
        <parameter name="version" value="127.0"/>
        <parameter name="platform" value="macOS Ventura"/>
        <classes>
            <class name="Capabilities.TestScenario1"/>
        </classes>
    </test>
</suite>
